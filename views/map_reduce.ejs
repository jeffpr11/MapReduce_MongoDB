<%- include("template/header", {title: title}); %>
<body>
  <div class="d-flex justify-content-start flex-wrap flex-md-nowrap pt-3 pb-2 mb-3 border-bottom">
    <a class="btn bg-transparent pt-2 mt-1" data-original-title="Volver" href="employment"><i class="fas fa-angle-left fa-2x"></i></a>
    <h1><%= title %></h1>
  </div>

  <div class="container-fluid text-center">
    <div class="row">
      <table class="table table-borderless">
        <thead class="table-dark">
          <tr>
            <th>Industria</th>
            <th>Salario Semanal Promedio</th>
          </tr>
        </thead>
        <tbody id="robot-row">
          <% for( var i = 0; i<limit ; i++ ){ %>
            <tr>
              <td class="text-start"><%= data[keys[i]].title %></td>
              <td><%= data[keys[i]].reduced.toFixed(2) %></td>
            </tr>
          <% } %>
        </tbody>
      </table>
  </div>
  <%- include("template/navegacion", {infoPage: infoPage}); %>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
    crossorigin="anonymous"></script>
</body>

</html>